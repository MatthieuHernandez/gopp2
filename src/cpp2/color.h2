Color: @enum<i8> type = {
    None := 0;
    Black := 1;
    White := 2;
}

otherColor:(in c: Color) -> Color = {
    if c == Color::Black {
        return Color::White;
    }
    else if c == Color::White {
        return Color::Black;
    }
    return Color::None;
}

 colorName:(in c: Color) -> std::string = {
    if c == Color::Black {
        return "Black";
    }
    else if c == Color::White {
        return "White";
    }
    return "None";
}
